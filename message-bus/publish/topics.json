/* 
User Create
Topic ID: USR1
Topic Name: user_create
Description: Create new user
FV Table: Users, UserGroup 
*/
{
    "ID": "1",                                  // Table: Users.ID2
    "userName": "johnsmith",
    "firstName": "John",                        // Table: Users.FirstName
    "middleName": "Jacob",                      // Table: Users.MiddleName
    "lastName": "Smith",                        // Table: Users.LastName
    "initials": "JJS",                          // Table: Users.Initials
    "email": "johnsmith@airliftnw.org",         // Table: Users.Email
    "UWNetID": "johnsmith",                     // *Not in FV but needed for auth*
    "groupID": "1",                             // *User.GroupID refers to FV User permissions, we want Pager Groups*
                                                // Potentially from PagableGroups or GroupOfUsers?
    "role": "Adult RN",                         // *Need to know Pilot, Flight RN, AOC, Ground Support*, shown as "Type" in FV UI
    "cellPhone": "(123) 123 1234",                   
    "qualificationID": "2"                      // Ex. Pilot PIC, Medical
}

