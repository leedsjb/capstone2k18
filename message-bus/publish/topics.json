/* 
Waypoint Created
Topic ID: WP1
Topic Name: waypoint_create
Description: New waypoint added to Flight Vector db by dispatcher
Waypoint types: agency, hospital, airport, landing zone
*/
{
    "ID": "1",                                      // Table: Waypoint.ID
    "notes": "FBO McCormicks, Ground tx: AMR",
    "name": "Yakima Valley Memorial Hospital",      // Table: Waypoint.Name
    "type": "hospital",                             // Table: Waypoint.Type
    "address1": "2811 Tieton Drive",
    "address2": "",
    "country": "United States",                     // Depends: AIRPORT.Country
    "state": "WA",                                  // Depends: AIRPORT.StateID 
    "county": "Yakima",                             // Depends: AIRPORT.County
    "city": "Yakima",                               // Depends: AIRPORT.City (maybe AIRPORT.CityID)
    "zip": "98902",                                 // Depends: AIRPORT.ZipID
    "lat": "48.433637",                             // Table: Waypoint.Latitude
    "long": "-123.406884",                          // Table: Waypoint.Longitude
    "GPSWaypoint": "227",
    "AirportIdentifier": "KBFI",                    // Table: AIRPORT.Ident
    "phone": [                                      // Depends: AIRPORT.Phone
        "2068901234",
        "(123) 123 4567 x123"
    ],
    "shortCode": "YV",
    "padTime": "00:06",                             // Dependent on type of waypoint
